#app
  width 1024px
  height 768px
  margin auto
  border 1px solid #000
#top
  height 65px
  margin auto
#top img
  width 100% 
#content
  width 100%
  height calc(768px - 65px - 20px)
#left
  width 215px
  height 100%
  float left
#right
  width 770px
  height 100%
  float right
#menu
  width 100%
  height calc(100% - 75px)
  font-family '標楷體'
  text-align center

  #club
    font-weight 500
    font-size 24px
    text-decoration underline
    color #ff00ff

  .item
    width 200px
    height 35px
    line-height 35px
    color black
    font-size 16px
    background  url('../public/img/menu1.png')
    margin-bottom 10px
    &:hover 
      background  url('../public/img/menu2.png')
#update
  width 100%
  height 25px
  text-align center
  font-family '標楷體'
  color black
  font-size 16px

#mar
  font-size 24px
  font-family '標楷體'
  color blue
  width 100%
  height 29px

#main
	width 100%
	height calc(100% - 50px)
	background  url('./0101.jpg')
	text-align center
	font-size 24px
	font-family '標楷體'
	font-weight bold
	font-style italic
	color blue

#footer
  font-family '標楷體'
  font-size 16px
  text-align center
  width 100%
  height 20px

